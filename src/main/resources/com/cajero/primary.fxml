<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="340.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cajero.PrimaryController">
   <children>
      <Label text="Cajero">
      <font>
         <Font name="System Bold" size="30.0" />
      </font>
      </Label>
      <Button fx:id="Ingresar" onAction="#switchToLogin" prefHeight="50.0" prefWidth="150.0" text="Ingresar" />
      <Button fx:id="CrearUsuario" mnemonicParsing="false" onAction="#switchToCreateUser" prefHeight="50.0" prefWidth="150.0" text="Crear Usuario" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
